                    <div class="row mplus-card">
                      <div class="col-md-auto mplus-space">
                          <h6 class="mplus-titles">Overall</h6>
                          <% if(currentMPlusScoreOverall < 400) { %>
                          <h6 class="mplus-scores mplus-stat-card"> <%=currentMPlusScoreOverall%> </h6>
                          <%}%>
                          <% if(currentMPlusScoreOverall < 700 && currentMPlusScoreOverall >= 500) { %>
                          <h6 class="mplus-scores mplus-stat-card" style="color:#43ff2a"> <%=currentMPlusScoreOverall%> </h6>
                          <%}%>
                          <% if(currentMPlusScoreOverall >= 700 && currentMPlusScoreOverall < 1000) { %>
                          <h6 class="mplus-scores mplus-stat-card" style="color:#15b300"> <%=currentMPlusScoreOverall%> </h6>
                          <%}%>
                          <% if(currentMPlusScoreOverall >= 1000 && currentMPlusScoreOverall < 1300) { %>
                          <h6 class="mplus-scores mplus-stat-card" style="color:#5565e2"> <%=currentMPlusScoreOverall%> </h6>
                          <%}%>
                          <% if(currentMPlusScoreOverall >= 1300 && currentMPlusScoreOverall < 1500 ) { %>
                          <h6 class="mplus-scores mplus-stat-card" style="color:#884ee9"> <%=currentMPlusScoreOverall%> </h6>
                          <%}%>
                          <% if(currentMPlusScoreOverall >= 1500 && currentMPlusScoreOverall < 1800) { %>
                          <h6 class="mplus-scores mplus-stat-card" style="color:#aa37e7"> <%=currentMPlusScoreOverall%> </h6>
                          <%}%>
                          <% if(currentMPlusScoreOverall >= 1800 && currentMPlusScoreOverall < 2800) { %>
                          <h6 class="mplus-scores mplus-stat-card" style="color:#eb6275"> <%=currentMPlusScoreOverall%> </h6>
                          <%}%>
                          <% if(currentMPlusScoreOverall >= 2800) { %>
                          <h6 class="mplus-scores mplus-stat-card" style="color:#ff8000"> <%=currentMPlusScoreOverall%> </h6>
                          <%}%>

                        </div>
                    <% if(currentMPlusScoreDPS >= 0) { %>
