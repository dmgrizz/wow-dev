                     <%  if(dungeons[x].id === mPlusHighest[i].map_challenge_mode_id){%>
                          <td> <img class=dungeon-img src="<%=dungeons[x].img%>" alt=""> </td>
                     <%  }%>
                     <% }%>
                     <td class="text-left dungeon-name"scope="row"><%=mPlusHighest[i].dungeon%></td>
                     <td class="dungeon-text"><%=mPlusHighest[i].mythic_level%></td>
                     <td class="dungeon-text"><%=mPlusHighest[i].score%></td>
                     <td class="dungeon-text"><%=localDateHighest[i]%></td>
                     <td class="dungeon-text"><%=highestTimes[i]%></td>
                     <td class="dungeon-text"><%=mPlusHighest[i].num_keystone_upgrades%></td>
                   </tr>
                   <% } %>
                 </tbody>
                </table>
            </div>
          </div>
        </div>
      </div>
</section>
