<%- include('partials/header') %>
    <img class="main-avatar" src="<%=avatarMain%>" alt="">
<div class="profile-body">
  <div class="header">
  <h1 id="wow-title">Wow Dashboard</h1>
  <h6 class="gold-text">Player Search</h6>
  <div class="dropdown">
    <form class="" action="/wowSearch" method="post">
    <input id="playerName" name="playerName" autocomplete="off" type="text" placeholder="Search.." oninput="myFunction()" onkeydown="filterFunction()" onkeyup="playerSearch()" >
    <div  id="myDropdown" class="dropdown-content">
      <!-- <input type="text" placeholder="Search.." id="myInput" onkeyup="filterFunction()"> -->
      <div class="dp-list">
        <h6 id="playerObj"></h6>
      </div>
    </div>
    <button class="btn btn-dark" type="submit" name="button">Go</button>
    </form>
  </div>
  <div class="header-btns">
    <button class="hd-btn-row" type="button" name="button"><span class="">Articles</span></button>
    <button class="hd-btn-row" type="button" name="button"><span class="">Rankings</span></button>
    <button class="hd-btn-row" type="button" name="button"><span class="">Profile</span></button>
    <a href="/"><button class="hd-btn-row" type="button" name="button"><span class="">Login</span></button></a>
  </div>
  </div>
  <div class="container-info">
    <div class="player-info">
      <div class="avatar-container">
          <div class="avatar-img">
          <% if(avatar){%>
                <img src="<%=avatar %>" alt="">
            <% }%>
          </div>
          <div class="top-info">   <!-- Avatar info DIV -->
              <h2 class="top-text gold-text"><%=characterName%></h2>
            <%if(guild){%>
                <h5 class="top-text text-indent gold-text"><%=guild%></h5>
            <%}%>
                <h5 class="top-text text-indent gold-text"><%=realm%></h5>
          </div>
      </div>
        <hr>
          <h4>Faction: <%=faction%></h4>
          <h4>Race: <%=race%></h4>
        <div class="char-div">
          <h4>Class: </h4> <h4 id="char"class="char-class"><%=characterClass%></h4>
        </div>
        <h4>Spec: <%=spec%></h4>
        <h4>Level: <%=level%></h4>
        <!-- <img src="https://render-us.worldofwarcraft.com/icons/56/inv_sword_39.jpg" alt=""> -->
        <h4>Achievement Points: <%=points%></h4>
        <h4>Item Level: <%=itemLevel%></h4>
    </div>
    <div class="pvp-info">
      <h2 class="gold-text">PVP Rating</h2>
      <div class="container">
        <div class="row">
          <div class="col-md-3 pvp-type">
            <h5 class="pvp-titles">2v2 Arena Rating</h5>
            <h4 class="pvp-scores"> <%=twoCr%> cr</h4>
          </div>
          <div class="col-md-3 pvp-type">
            <h5 class="pvp-titles">3v3 Arena Rating</h5>
            <h4 class="pvp-scores"> <%=threeRating%> cr</h4>
          </div>
          <div class="col-md-3 pvp-type">
            <h5 class="pvp-titles">RBG Arena Rating</h5>
            <h4 class="pvp-scores"><%=rbgCr%> cr</h4>
          </div>
        </div>
        <div class="row">
          <div class="col-md-3 twos-pvp pvp-matches" onmouseover="showTwosGames()" onmouseout="hideTwosGames()">
            <div class="">
            <h6 class="pvp-titles">2v2 Win Rate %</h6>
              <%if(twoWinRate >= 50){%>
                <h5 class="pvp-scores" style="color:green"><%=twoWinRate%>%</h5>
              <%} else {%>
                <h5 class="pvp-scores" style="color:red"><%=twoWinRate%>%</h5>
              <%}%>
            </div>
            <div id="twos-tool" class="pvp-tool-tips hide">
              <div class="pvp-games">
                <div class="game-won">
                  <h5 class="pvp-scores">Wins: </h5>
                  <h5 class="pvp-scores"><%=twoWins%></h5>
                </div>
                <div class="game-won">
                  <h5 class="pvp-scores">Losses: </h5>
                  <h5 class="pvp-scores"><%=twoLost%></h5>
                </div>
                <div class="game-won">
                  <h5 class="pvp-scores">Total: </h5>
                  <h5 class="pvp-scores"><%=twoMatches%></h5>
                </div>
                <div class="game-won">
                    <h5 class="pvp-won-tp">Win:</h5>
                    <h5 class="pvp-won-tp"><%=twoWinRate%>%</h5>
                </div>
              </div>
            </div>
          </div>
          <div class="col-md-3 pvp-matches" onmouseover="showThreesGames()" onmouseout="hideThreesGames()">
            <div class="">
              <h6 class="pvp-titles">3v3 Win Rate %</h6>
              <%if(threeWinRate >= 50){%>
                <h4 class="pvp-scores" style="color:green"><%=threeWinRate%>%</h4>
              <%} else  {%>
                <h4 class="pvp-scores" style="color:red"><%=threeWinRate%>%</h4>
              <%}%>
              <div id="threes-tool" class="pvp-tool-tips hide">
                <div class="pvp-games">
                  <div class="game-won">
                      <h5 class="pvp-scores">Wins: </h5>
                      <h5 class="pvp-scores"><%=threeWins%></h5>
                  </div>
                  <div class="game-won">
                      <h5 class="pvp-scores">Losses: </h5>
                      <h5 class="pvp-scores"><%=threeLost%></h5>
                  </div>
                  <div class="game-won">
                      <h5 class="pvp-scores">Total: </h5>
                      <h5 class="pvp-scores"><%=threeMatches%></h5>
                  </div>
                  <div class="game-won">
                      <h5 class="pvp-won-tp">Win:</h5>
                      <h5 class="pvp-won-tp"><%=threeWinRate%>%</h5>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-md-3 pvp-matches" onmouseover="showRbgGames()" onmouseout="hideRbgGames()">
            <div class="">
              <h6 class="pvp-titles">BG Win Rate %</h6>
              <%if(rbgWinRate >= 50){%>
              <h4 class="pvp-scores" style="color:green"><%=rbgWinRate%>%</h4>
              <%} else {%>
                <h4 class="pvp-scores" style="color:red"><%=rbgWinRate%>%</h4>
              <%}%>
              <div id="rbg-tool" class="pvp-tool-tips hide">
                <div class="pvp-games">
                  <div class="game-won">
                      <h5 class="pvp-scores">Wins: </h5>
                      <h5 class="pvp-scores"><%=rbgWins%></h5>
                  </div>
                  <div class="game-won">
                      <h5 class="pvp-scores">Losses: </h5>
                      <h5 class="pvp-scores"><%=rbgLost%></h5>
                  </div>
                  <div class="game-won">
                      <h5 class="pvp-scores">Total: </h5>
                      <h5 class="pvp-scores"><%=rbgMatches%></h5>
                  </div>
                  <div class="game-won">
                      <h5 class="pvp-won-tp">Win:</h5>
                      <h5 class="pvp-won-tp"><%=rbgWinRate%>%</h5>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-3 pvp-matches hide">
            <h6 class="pvp-titles">BG Matches Played</h6>
            <h5 class="pvp-scores"><%=rbgMatches%></h5>
            <div class="pvp-games">
              <div class="game-won">
                <h6 class="pvp-titles-small">Won</h6>
                <h5 class="pvp-scores"><%=rbgWins%></h5>
              </div>
              <div class="game-lost">
                <h6 class="pvp-titles-small">Lost</h6>
                <h5 class="pvp-scores"><%=rbgLost%></h5>
              </div>
            </div>
          </div>
        </div>
      </div>
      <br>
      <div class="pvp-talent-info">
        <h2 class="gold-text">PVP Talents</h2>
        <div class="talent-container">
            <%for (var i = 0; i < pvpTal.length; i++) {%>
              <div class="pvp-tal-img-container">
                <div class="tal-inner">
                  <a class="tal-name" href="#" data-wowhead="spell=<%=pvpTal[i]%>"></a>
                </div>
              </div>
              <%}%>
        </div>
      </div>
    </div>
    <div class="talent-info">
      <h2 class="gold-text">Talents</h2>
      <div class="talent-container">
          <%for (var i = 0; i < spellIds.length; i++) {%>
            <div class="tal-img-container">
              <div class="tal-inner">
                <a class="tal-name" href="#" data-wowhead="spell=<%=spellIds[i]%>"></a>
              </div>
            </div>
            <%}%>

      </div>

      <br>
    </div>

    <br>
  </div>

<div class="container-info">
  <%if(guild){%>
        <div onmouseover="onMouseOver()" onmouseout="onMouseOut()" class="guild">
        <h3 class="gold-text">Guild - <%=guild%> </h3>
          <div class="guild-info">
            <h6> Members: <%=guildMembers%> </h6>
            <h6 class="text-indent"> AP: <%=guildPoints%></h6>
          </div>
          <hr>
          <div id="scroll-arrow" class="new-arrow">
            <img src="/arrow-down-bold-box(1).png" alt="">
          </div>
          <!-- <div id="scroll-arrow" class="arrow">
                <span></span>
                <span></span>
                <span></span>
          </div> -->
          <div class="roster">
        <%for (var i = 0; i < guildRoster.length; i++) { %>
        <h6> <%= guildRoster[i] %></h6>
        <%}%>
          </div>
        </div>
    <%}%>
        <%for(var i=0; i < equipmentSlot.length; i++){%>
          <div class="equipment">
            <h4 class="gold-text">Equipment</h4>
            <div class="row">
            <%for(var i=0; i < equipmentSlot.length; i++){%>
              <%for(var i=0; i < equipmentSlot.length; i++){%>
            <div class="col-md-3">
              <div class="item-header">
                <h5><%=equipmentSlot[i]%> - </h5>
                  <p class="quality"><%=equipmentQuality[i]%></p>
              </div>
              <br>
              <div class="item-media">
                <a class="q3" data-wowhead="" href=<%=equipmentImages[i]%> >Item image</a>
                </div>
              </div>
                <%}%>
              <%}%>
              </div>
            </div>
          <%}%>
        <div class="talent-info">

          </div>
</div>

</div>
<%- include('partials/footer') %>
